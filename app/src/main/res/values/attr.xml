<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OtpView">
        <attr name="android:inputType" />
        <attr name="android:textColor" />
        <attr name="text_background_color" format="color" />
        <attr name="otp" format="string" />
    </declare-styleable>
    <declare-styleable name="LinearTimerView">
        <attr name="radius" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="initialColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="startingPoint" format="integer" />
        <attr name="preFillPoint" format="integer" />
    </declare-styleable>
</resources>